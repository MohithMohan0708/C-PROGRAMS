#include<stdio.h>
int main()
{
    int i,j,n=5;
    for(i=1;i<=n;i++)
    {
      for(j=n;j>i;j--) 
      {
        printf(" ");
      } 
    for (j=0;j<i;j++)
    {
        printf("%d",i+j);
    }
    for (j=i-1;j>=1;j--)
    {
        printf("%d",j+i-1);
    }
    printf("\n");
    }
    return 0;
}
